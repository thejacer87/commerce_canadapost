{
    "name": "drupal/commerce_canadapost",
    "type": "drupal-module",
    "description": "A Drupal 8 module providing integration features with Canada Post and Commerce.",
    "keywords": ["Drupal"],
    "license": "GPL-2.0+",
    "homepage": "https://www.drupal.org/project/commerce_canadapost",
    "support": {
        "issues": "https://www.drupal.org/project/issues/commerce_canadapost",
        "source": "http://cgit.drupalcode.org/commerce_canadapost"
    },
    "require": {
        "drupal/commerce_shipping": "^8.2",
        "openlss/lib-array2xml": "^0.5.1",
        "thejacer87/canadapost-api": "^0.2.4"
    },
    "minimum-stability": "dev",
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        },
        "patches": {
            "drupal/commerce_shipping": {
                "Add Shipments tab to Order page": "https://www.drupal.org/files/issues/2018-06-27/commerce_shipping-shipments-tab-2981945-2.patch"
            }
        }
    }
}
